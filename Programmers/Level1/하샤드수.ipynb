   "source": [
    "def solution(x):\n",
    "    total=0\n",
    "    num=str(x)\n",
    "    for i in num:\n",
    "        total+=int(i)\n",
    "    if x%total==0:\n",
    "        return True\n",
    "    return False"
   ]
