    "cnt=0\n",
    "x=1; y=1\n",
    "for _ in range(N*M):\n",
    "    cnt+=1\n",
    "    if x==N and y==M:\n",
    "        break\n",
    "    if y==M or rect[(x-1)][y]==0:\n",
    "        x+=1\n",
    "    else:\n",
    "        y+=1\n",
    "    \n",
    "print(cnt)"
   ]
